{
  "title": "MultiUI Component Schema",
  "description": "Schema for defining a new MultiUI Component.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the component."
    },
    "description": {
      "type": "string",
      "description": "A description of the component."
    },
    "version": {
      "type": "string",
      "description": "The version of the component."
    },
    "author": {
      "type": "string",
      "description": "The author of the component."
    },
    "website": {
      "type": "string",
      "description": "The website of the component."
    },
    "dependencies": {
      "type": "array",
      "description": "An array of dependencies for the component.",
      "items": {
        "type": "string"
      }
    }
  }
}
